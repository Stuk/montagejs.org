{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
              "../node_modules/montage/composer/"
          , "../node_modules/montage/core/"
          , "../node_modules/montage/ui/"
          , "../node_modules/montage/window-loader/"

        ],
        "exclude": [
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/)_"

    },
    "plugins": [ "plugins/markdown" ],
    "markdown": {
        "parser": "marked"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./template",
        "encoding": "utf8",
        "destination": "../../apis",
        "recurse": true,
        "tutorials": "./template",
        "query": "value",
        "private": false,
        "lenient": true
    },
    "jsVersion": 180
}
